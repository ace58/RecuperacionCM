        app:layout_constraintTop_toBottomOf="@+id/elugar" />

    <EditText
        android:id="@+id/efecha2"
        android:layout_width="196dp"
        android:layout_height="64dp"
        android:layout_marginTop="324dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.874"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/elugar" />

    <Button
        android:id="@+id/bhora2"
        android:layout_width="143dp"
        android:layout_height="39dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="340dp"
        android:onClick="onClick"
        android:text="HORA"
        app:layout_constraintEnd_toStartOf="@+id/efecha"
        app:layout_constraintHorizontal_bias="0.288"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bfecha" />

    <EditText
        android:id="@+id/ehora2"
        android:layout_width="200dp"
        android:layout_height="46dp"
        android:layout_marginTop="328dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.872"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/efecha" />

    <Button
        android:id="@+id/blugar"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_marginTop="28dp"
        android:onClick="Actividad_Maps"
        android:text="Lugar"
        app:layout_constraintEnd_toStartOf="@+id/elugar"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/punto_recogida" />

    <Button
        android:id="@+id/blugar2"
        android:layout_width="140dp"
        android:layout_height="40dp"
        android:layout_marginTop="336dp"
        android:onClick="Actividad_Maps"
        android:text="Lugar"
        app:layout_constraintEnd_toStartOf="@+id/elugar"
        app:layout_constraintHorizontal_bias="0.379"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/punto_recogida" />

</androidx.constraintlayout.widget.ConstraintLayout>